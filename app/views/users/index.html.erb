<h1>Listing users</h1>

<table class="table table-striped">
  <thead>
  	<tr>
		<td>User</td>
		<td>Joined</td>
    </tr>
  </thead>
<% @users.each do |user| %>
  <tr>
	<td><%= user.name %></td>
	<td><%= user.created_at.strftime("%m/%d/%Y") %></td>
  </tr>
<% end %>
</table>

